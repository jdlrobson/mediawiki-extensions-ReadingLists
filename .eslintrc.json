{
	"root": true,
	"extends": [
		"wikimedia",
		"wikimedia/mediawiki"
	],
	"env": {
		"commonjs": true
	},
	"rules": {
		"jsdoc/no-undefined-types": "off",
		"compat/compat": "off",
		"no-implicit-globals": "off"
	},
	"overrides": [
		{
			"files": "**/*.vue",
			"extends": "wikimedia/vue-es6"
		}
	]
}
